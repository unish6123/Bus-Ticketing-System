   "source": [
    "from datetime import date\n",
    "\n",
    "# Get today's date\n",
    "today = date.today()\n",
    "\n",
    "# Format today's date as mm/dd/yyyy\n",
    "formatted_date = today.strftime(\"%m/%d/%Y\")\n",
    "\n",
    "# Print the formatted date\n",
    "print(\"Today's date (mm/dd/yyyy):\", formatted_date)\n",
    "\n"
   ]
  }
